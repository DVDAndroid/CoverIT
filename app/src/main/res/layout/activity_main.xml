<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e0e0e0"
    tools:context="com.stefanodecillis.intcoverage.MainActivity">

    <View
        android:id="@+id/view5"
        android:layout_width="200dp"
        android:layout_height="2dp"
        android:background="#FF909090"
        app:layout_constraintHorizontal_bias="0.165"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/autocompleteNum" />

    <AutoCompleteTextView
        android:id="@+id/autocompleteNum"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="#e0e0e0"
        android:hint="Numero Civico"
        app:layout_constraintHorizontal_bias="0.172"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view4" />

    <View
        android:id="@+id/view2"
        android:layout_width="340dp"
        android:layout_height="2dp"
        android:background="#FF909090"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/autocomplete" />

    <AutoCompleteTextView
        android:id="@+id/autocompleteCity"
        android:layout_width="340dp"
        android:layout_height="40dp"
        android:background="#e0e0e0"
        android:hint="CittÃ "
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <View
        android:id="@+id/view3"
        android:layout_width="340dp"
        android:layout_height="2dp"
        android:background="#FF909090"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/autocompleteCity" />

    <View
        android:id="@+id/view4"
        android:layout_width="340dp"
        android:layout_height="2dp"
        android:background="#FF909090"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/autocompleteAddr" />

    <AutoCompleteTextView
        android:id="@+id/autocompleteAddr"
        android:layout_width="340dp"
        android:layout_height="40dp"
        android:background="#e0e0e0"
        android:hint="Via/piazza/Corso.."
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view3" />

    <AutoCompleteTextView
        android:id="@+id/autocomplete"
        android:layout_width="340dp"
        android:layout_height="39dp"
        android:layout_marginTop="79dp"
        android:background="#e0e0e0"
        android:hint="Provincia"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="53dp"
        android:adjustViewBounds="false"
        android:background="#73e8ff"
        android:cropToPadding="false"
        android:scaleType="fitXY"
        android:visibility="invisible"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:background="#eeeeee"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:clipChildren="false"
        android:clipToPadding="false"
        app:layout_anchorGravity="bottom|right|end"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        android:layout_marginRight="36dp"
        android:layout_marginTop="66dp"
        app:layout_constraintTop_toBottomOf="@+id/view5">

        <com.github.jorgecastilloprz.FABProgressCircle
            android:id="@+id/fabProgressCircle"
            android:layout_width="80dp"
            android:layout_height="80dp"
            app:arcColor="@color/fab_orange_light"
            app:finalIcon="@drawable/ic_done"
            app:reusable="true">

            <android.support.design.widget.FloatingActionButton
                android:id="@+id/findBtn"
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:text="Button"
                app:backgroundTint="#81c784"
                app:borderWidth="0dp"
                app:srcCompat="@android:drawable/ic_menu_search" />
        </com.github.jorgecastilloprz.FABProgressCircle>
    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
